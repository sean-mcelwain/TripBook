     res.items.forEach((imageRef) => {\r\n          imageRef.getDownloadURL().then((url) => {\r\n            if (allImages.indexOf(url) === -1) {\r\n              setImages((allImages) => [...allImages, url]);\r\n            }\r\n          });\r\n        });\r\n      })\r\n      .catch(function (error) {\r\n        console.log(error);\r\n      });\r\n  };\r\n\r\n  const deleteFromFirebase = (url) => {\r\n    let pictureRef = storage.refFromURL(url);\r\n    pictureRef\r\n      .delete()\r\n      .then(() => {\r\n        setImages(allImages.filter((image) => image !== url));\r\n        alert(\"Picture is successfully deleted!\");\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  };\r\n\r\n  return (\r\n    <div>\r\n      (\r\n      <header className=\"App-header\">\r\n        <button onClick={() => getFromFirebase()}>\r\n          Get Images from Firebase\r\n        </button>\r\n\r\n        <div id=\"photos\">\r\n          {allImages.map((image) => {\r\n            return (\r\n              <div key={image} className=\"image\">\r\n                <img src={image} alt=\"\" />\r\n                <button onClick={() => deleteFromFirebase(image)}>\r\n                  Delete\r\n                </button>\r\n              </div>\r\n            );\r\n          })}\r\n        </div>\r\n      </header>\r\n      )\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default AllImages;\r\n","D:\\Users\\McElwain\\Documents\\Education\\KU\\KU Coding Boot Camp\\GitBash\\TripBook\\client\\src\\components\\ImageUpload\\firebase.js",[],"D:\\Users\\McElwain\\Documents\\Education\\KU\\KU Coding Boot Camp\\GitBash\\TripBook\\client\\src\\components\\TripList\\index.js",["127"],"import React from 'react';\r\nimport { Link } from 'react-router-dom';\r\n\r\nconst UserTripList = ({ trips, title }) => {\r\n  if (!trips.length) {\r\n    return <h3>No Trips Yet</h3>;\r\n  }\r\n\r\n  return (\r\n    <div>\r\n      <h3>{title}</h3>\r\n      {trips &&\r\n        trips.map((